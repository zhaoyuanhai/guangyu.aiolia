
@{
    ViewBag.Title = "AddHomeImage";
}

@using (Html.BeginForm("AddHomeImage", "Manager", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div>
        <form class="form-inline" action="@Url.Action("AddThreeModule")" role="form">
            <div class="form-group">
                <label class="sr-only" for="inputfile">文件输入</label>
                <input type="file" name="file" id="inputfile">
            </div>
            <button type="submit" class="btn btn-default">提交</button>
        </form>
    </div>

}

